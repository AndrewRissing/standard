<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
	targetNamespace="http://cufxstandards.com/v2/ValidationStatus.xsd"
	version="2.0.$Revision$"
	elementFormDefault="qualified"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns="http://cufxstandards.com/v2/ValidationStatus.xsd">

	<xsd:simpleType name = "ValidationStatus">
    <xsd:annotation>
      <xsd:documentation>Enumeration defining possible results of a simple validation.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base = "xsd:string">
    	<xsd:enumeration value = "Pass">
       		<xsd:annotation>
       			<xsd:documentation>
       				The applicant is valid.
       	 		</xsd:documentation>
       		</xsd:annotation>
      	</xsd:enumeration>
      <xsd:enumeration value = "Fail">
       		<xsd:annotation>
       			<xsd:documentation>
       				The applicant is not valid.
       	 		</xsd:documentation>
       		</xsd:annotation>
      	</xsd:enumeration>
      <xsd:enumeration value = "Indeterminate">
       		<xsd:annotation>
       			<xsd:documentation>
       				It could not be determined if the applicant is valid or not.
       	 		</xsd:documentation>
       		</xsd:annotation>
      	</xsd:enumeration>
      <xsd:enumeration value = "Error">
       		<xsd:annotation>
       			<xsd:documentation>
       				There was an error in the validation process.
       	 		</xsd:documentation>
       		</xsd:annotation>
      	</xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>

</xsd:schema>