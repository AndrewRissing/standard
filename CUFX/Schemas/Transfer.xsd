					<xs:documentation>The DueDate the user entered for this transfer</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProjectedDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The date the transfer will actually get processed by the system</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Amount" type="xs:decimal" minOccurs="0" />
      <xs:element name="fromAccount" type="account:Account" minOccurs ="0" maxOccurs ="1"/>
      <xs:element name="toAccount" type="account:Account" minOccurs ="0" maxOccurs ="1"/>
