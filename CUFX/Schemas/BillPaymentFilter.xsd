  </xs:element>

  <xs:complexType name="BillPaymentFilter" >
    <xs:complexContent>
      <xs:annotation>
        <xs:documentation>
          The list of filter options for services requesting bill payment data.
        </xs:documentation>
      </xs:annotation>
      <xs:extension base="fundsTransferFilterBase:FundsTransferFilterBase">
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

</xs:schema>
